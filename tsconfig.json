{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true, // habilita arquivos de declaração
    "outDir": "dist/", // após compilada
    "lib": ["es7", "dom", "ES2017.Object"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/"]
}
